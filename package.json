{
  "name": "@altertech/jsaltt",
  "version": "0.1.3",
  "description": "TypeScript tools",
  "scripts": {
    "dev": "vite",
    "build": "tsc src/*.ts --declaration --emitDeclarationOnly --declarationDir dist/ && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alttch/jsaltt.git"
  },
  "keywords": [
    "tools",
    "logging"
  ],
  "author": "Altertech",
  "files":["dist"],
  "types": "./lib.d.ts",
  "main":"./dist/jsaltt.umd.js",
  "module":"./dist/jsaltt.es.js",
  "exports": {
    ".": {
      "import": "./dist/jsaltt.es.js",
      "require": "./dist/jsaltt.umd.js"
    }
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/alttch/jsaltt/issues"
  },
  "dependencies": {
    "chalk": "^5.2.0"
  },
  "homepage": "https://github.com/alttch/jsaltt",
  "devDependencies": {
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
